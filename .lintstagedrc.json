{
  "*.ts": ["prettier --write", "npm run lint_fix", "npm run lint", "npx cspell lint --no-must-find-files"],
  "*.js": ["prettier --write", "npm run lint_fix", "npm run lint", "npx cspell lint --no-must-find-files"],
  "*.json": ["prettier --write", "npx cspell lint --no-must-find-files"],
  "*.sh": ["npx cspell lint --no-must-find-files"],
  "*.groovy": ["npm-groovy-lint -l warning --fix --failon warning", "npx cspell lint --no-must-find-files"]
}