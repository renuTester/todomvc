{
	"name": "todomvc",
	"version": "1.0.0",
	"description": "UI Testing of ToDo MVC App",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"cyopen": "cypress open",
		"cy:run:all:chrome": "cypress run --browser chrome; echo \"Testing complete.\"",
		"cy:run:spec": "cypress run --browser chrome --spec",
		"cypress_run_tests": "npm run cleanup && node cypress_runner.ts --config-file cypress.config.ts --spec",
		"code-quality-win": "npm run knip & snyk test --dev --severity-threshold=critical & npm audit --json --severity=critical",
    "code-quality-mac": "npm run knip; snyk test --dev --severity-threshold=critical; npm audit",
		"knip": "knip",
		"prepare": "husky",
		"setup": "git checkout main && npm install",
		"cy:run": "nx cy:run e2e",
		"deduplicate": "npm dedupe",
		"format": "prettier --write .",
		"nuke": "git clean -xdf; npm install",
		"prettier": "prettier --check .",
		"open-report": "start ./mochawesome-report/mochawesome.html",
		"open-report-linux": "xdg-open cypress/mochawesome-report/mochawesome.html",
		"open-report-mac": "open cypress/mochawesome-report/mochawesome.html"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"UI",
		"Test",
		"Automation"
	],
	"author": "Yadav",
	"license": "ISC",
	"devDependencies": {
		"@cypress-audit/lighthouse": "^1.4.2",
		"@eslint/js": "^9.17.0",
		"@testing-library/cypress": "9.0.0",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"axe-core": "^4.10.2",
		"cspell": "^8.3.2",
		"cypress": "12.17.0",
		"cypress-axe": "^1.5.0",
		"cypress-multi-reporters": "^1.2.4",
		"cypress-plugin-tab": "^1.0.5",
		"cypress-tags": "^1.2.2",
		"cypress-xpath": "^1.4.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-chai-friendly": "0.7.2",
		"eslint-plugin-cypress": "2.12.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-module-resolver": "1.5.0",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-no-unsanitized": "4.1.2",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-simple-import-sort": "8.0.0",
		"eslint-plugin-testing-library": "7.1.1",
		"globals": "^15.14.0",
		"husky": "^9.0.11",
		"junit-merge": "^2.0.0",
		"knip": "^5.41.1",
		"lint-staged": "^13.2.2",
		"mocha": "^7.1.1",
		"mocha-junit-reporter": "^1.23.3",
		"mochawesome": "^5.0.0",
		"mochawesome-merge": "^4.0.3",
		"mochawesome-report-generator": "^4.1.0",
		"npm": "^6.14.4",
		"prettier": "^3.4.2",
		"snyk": "^1.1294.3",
		"typescript": "^5.0.4",
		"typescript-eslint": "^8.19.0",
		"yargs": "^15.3.1"
	}
}
