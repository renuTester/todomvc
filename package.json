{
  "name": "todomvc",
  "version": "1.0.0",
  "description": "UI Testing of ToDo MVC App",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "cypress open",
    "cy:execute:all:chrome": "cypress run --browser chrome",
    "cyopen": "cypress open",
    "cy:run:all:chrome": "cypress run --browser chrome; echo \"Testing complete.\"",
    "cy:run:spec": "cypress run --browser chrome --spec",
    "cleanup": "",
    "merge_junit_reports": "junit-merge -d junit_report -o junit_report/merged.xml",
    "merge_mochawesome_reports": "mochawesome-merge mochawesome-report/*.json > mochawesome-report/output_merged.json",
    "generate_mochawesome_report": "marge mochawesome-report/output_merged.json",
    "cypress_run_tests_without_node": "npm run cleanup; cypress run --browser chrome --reporter cypress-multi-reporters --reporter-options configFile=config.json; npm run merge_junit_reports; npm run merge_mochawesome_reports; npm run generate_mochawesome_report",
    
    "cypress_run_tests": "npm run cleanup && node cypress_runner.ts --config-file cypress.config.ts --spec",
    "cypress_run_tests_func_other_two": "npm run cleanup && node cypress_runner.js --config-file cypress_func_other_two.config.ts --spec",
    "cypress_run_tests_func_other_three": "npm run cleanup && node cypress_runner.js --config-file cypress_func_other_three.config.ts --spec",
    "cypress_run_tests_func_other_four": "npm run cleanup && node cypress_runner.js --config-file cypress_func_other_four.config.ts --spec",
    "cypress_run_tests_func_other_custom": "npm run cleanup && node cypress_runner.js --config-file cypress_func_other_custom_run.config.ts --spec",
    "cypress_run_tests_staging": "npm run cleanup && node cypress_runner.js --config-file cypress_staging.config.ts --spec",
    "lint": "eslint --ignore-pattern node_modules/",
    "lint_fix": "eslint --fix --ignore-pattern node_modules/",
    "lint_groovy": "npm-groovy-lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renuTester/todomvc.git"
  },
  "keywords": [
    "UI",
    "Test",
    "Automation"
  ],
  "author": "Yadav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/renuTester/todomvc/issues"
  },
  "homepage": "https://github.com/renuTester/todomvc#readme",
  "devDependencies": {
    "cypress": "12.17.0",
    "@testing-library/cypress": "9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "cspell": "^8.3.2",
    "cypress-failed-log": "^2.6.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.2.4",
    "cypress-pipe": "2.0.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-recurse": "^1.23.0",
    "cypress-wait-until": "^1.6.1",
    "cypress-xpath": "^1.4.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-module-resolver": "1.5.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-testing-library": "5.9.1",
    "husky": "^9.0.11",
    "junit-merge": "^2.0.0",
    "lint-staged": "^13.2.2",
    "mocha": "^7.1.1",
    "mocha-junit-reporter": "^1.23.3",
    "mochawesome": "^5.0.0",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^4.1.0",
    "npm": "^6.14.4",
    "npm-groovy-lint": "^14.2.3",
    "prettier": "2.8.3",
    "typescript": "^4.9.5",
    "yargs": "^15.3.1",
    "@cypress-audit/lighthouse": "^1.4.2",
    "axe-core": "^4.10.2",
    "cypress-axe": "^1.5.0",
    "cypress-tags": "^1.2.2"
  }
}
